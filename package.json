    "@poppinss/utils": "^2.1.2",
    "apollo-server": "^2.10.1",
    "camelcase": "^5.3.1",
    "chance": "^1.1.4",
    "cli-table3": "^0.5.1",
    "glob": "^7.1.6",
    "graphql-query-complexity": "^0.4.1",
    "graphql-subscriptions": "^1.1.0",
    "graphql-tools": "^4.0.7",
    "lodash": "^4.17.15",
    "log-update": "^4.0.0",
    "macroable": "^4.0.2",
    "moment": "^2.24.0",
    "php-serialize": "^4.0.2",
    "pretty-hrtime": "^1.0.3",
    "randomstring": "^1.1.5",
    "reflect-metadata": "^0.1.13",
    "snake-case": "^3.0.3",
    "uuid": "^7.0.3",
    "validatorjs": "^3.18.1"
  },
  "devDependencies": {
    "@tngraphql/console": "^1.0.1",
